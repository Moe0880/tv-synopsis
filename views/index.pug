html
   
    body
        h1= showdata.title
            h2 Developed By:
            ul
                for name in showdata.developedBy
                    li= name

            h2 Starring:
            ul
                for star in showdata.starring
                    li= star
            h2 Seasons
            ul
                for season in showdata.seasons
                  li  
                    a(href=`/seasons/${season.number}`)= `Season ${season.number}`
                
           
